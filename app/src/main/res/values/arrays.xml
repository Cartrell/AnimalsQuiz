    <!-- =========================== -->
    <!-- 1. question arthropod -->
    <!-- =========================== -->
    <!-- id marker of question text. used as a delimiter between questions-->
    <!-- must be first pair of tags of each question -->
    <!-- required tags -->
    <item>questionId</item>
    <item>arthropod</item>

    <!-- string id of question text. -->
    <!-- required tags -->
    <item>questionTextStringId</item>
    <item>@string/question_arthropod</item>
    
    <!-- question type. required -->
    <!-- can be "radio", "check", "edit" -->
    <!-- required tags -->
    <item>questionType</item>
    <item>radio</item>
    
    <!-- resource ID of an image if one is shown with the question -->
    <!-- optional tags -->
    <!--<item>questionImageId</item>
    <item></item>-->
    
    <!-- number of incorrect choices -->
    <!-- first tag: number of incorrect choices -->
    <!-- second tag: an <item> tag that specifies the resource String ID of the choice text -->
    <!-- the number of resource ID <item> tags must match the value specified in the first tag -->
    <!-- only required for question types "radio" and "check"; not used for other question types -->
    <item>incorrectChoices</item>
    <item>3</item>
    <item>@string/choice_arthropod_eagle</item>
    <item>@string/choice_arthropod_monkey</item>
    <item>@string/choice_arthropod_amphibian</item>
    
    <!-- number of correct choices -->
    <!-- required tags -->
    <item>correctChoices</item>
    <item>1</item>
    <item>@string/choice_arthropod_spider</item>

